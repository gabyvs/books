{
  "name": "books",
  "version": "0.1.0",
  "description": "Angular 2 SPA that manages a list of Books",
  "scripts": {
    "build": "catufol -b",
    "start:neza": "node node_modules/nezaldi --conf=devserver-config.json",
    "start:static": "http-server ./build",
    "start:prod": "npm run start:static & npm run start:neza",
    "start": "catufol -r & npm run start:neza",
    "test:debug": "catufol -i",
    "test": "catufol -t",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gabyvs/books.git"
  },
  "keywords": [
    "Angular2"
  ],
  "author": "Gabriela Vazquez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gabyvs/books/issues"
  },
  "homepage": "https://github.com/gabyvs/books#readme",
  "devDependencies": {
    "catufol": "0.1.4",
    "html-loader": "0.4.4",
    "http-server": "0.9.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "nezaldi": "0.2.9",
    "remap-istanbul": "0.6.4",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "typings":"1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/router": "3.0.1",
    "@angular/upgrade": "2.0.1",
    "angular-in-memory-web-api": "0.1.1",
    "bootstrap": "3.3.7",
    "es6-shim": "0.35.1",
    "lodash": "4.16.2",
    "moment": "2.15.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  }
}
